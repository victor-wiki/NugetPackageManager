<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>    
    <add key="Source" value="https://api.nuget.org/v3/index.json"/>
    <add key="PushArgs" value="{PackageFileName} -k {Key} -s {Source}"/>
    <add key="DeleteArgs" value="{PackageName} {Version} -k {Key} -s {Source}"/>
  </appSettings>
</configuration>